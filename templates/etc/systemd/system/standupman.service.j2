[Unit]
Description=StandupMan Service
Wants=network.target
After=network.target

[Service]
User={{ standupman_user }}
Group={{ standupman_group | d(standupman_user) }}
WorkingDirectory={{ standupman_webroot }}
ExecStart=/usr/bin/dotnet StandupMan.dll
Restart=always

[Install]
WantedBy=multi-user.target
